CREATE TABLE DETAIL_STDKEVI4(
    CUSTOMER_NO VARCHAR2(25),
    CUSTOMER_ID VARCHAR2(25),
    
    SUBJECT VARCHAR2(35),
    DETAIL_DESCRIPTION VARCHAR2(105),
    DETAIL_USER VARCHAR2(35),
    D_DATE DATE,
    PRIMARY KEY (CUSTOMER_NO,CUSTOMER_ID),
    
    CONSTRAINT FK_DETAIL_STDKEVI4 FOREIGN KEY (CUSTOMER_NO)
    REFERENCES MASTER_STDKEVI4 (CUSTOMER_NO)
);

CREATE SYNONYM DETAILS_STDKEVI4 FOR DETAIL_STDKEVI4;