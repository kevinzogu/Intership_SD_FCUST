CREATE TABLE DETAIL_STDKEVI4(
    CUSTOMER_REFERENCE VARCHAR2(25),
    CUSTOMER_ID VARCHAR2(25),
    
    SUBJECT VARCHAR2(35),
    DETAIL_DESCRIPTION VARCHAR2(105),
    DETAIL_USER VARCHAR2(35),
    D_DATE DATE,
    PRIMARY KEY (CUSTOMER_REFERENCE,CUSTOMER_ID),
    
    CONSTRAINT FK_DETAIL_STDKEVI4 FOREIGN KEY (CUSTOMER_REFERENCE)
    REFERENCES MASTER_STDKEVI4 (CUSTOMER_REFERENCE)
);

CREATE SYNONYM DETAILS_STDKEVI4 FOR DETAIL_STDKEVI4;